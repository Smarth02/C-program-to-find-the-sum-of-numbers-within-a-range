#include <stdio.h>
int main()
{
    int roll_num[10], marks[10];
    printf("Enter the roll numbers ");
    for(int i=0;i<10;i++)
        scanf("%d", &roll_num[i]);
    printf("Enter marks ");
    for(int l=0;l<10;l++)
        scanf("%d", &marks[l]);
    int x, y;
    y=1;
    printf("Enter the roll number to search ");
    scanf("%d", &x);
    for(int j=0;j<10;j++)
    {
        if(roll_num[j]==x){
        printf("%d", marks[j]);
        y=0;
        }
    }
    if(y)
        printf("Entry not found");
    return 0;
}
